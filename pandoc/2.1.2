#%Module1.0#####################################################################
##
## pandoc modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tpandoc - Adds pandoc to your path"
}

module-whatis   "pandoc-1.19.2.4"

set      iigb_modules    $::env(IIGB_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set     version         3.2.9
set     prefix          ${modules_dir}/${version}
set     exec_prefix     ${prefix}
set     datarootdir     ${prefix}/share

#module load texlive

prepend-path    PATH            ${iigb_modules}/pandoc/2.1.2/bin

${modules_dir}

